# Git
alias g="git"
alias add="git add"
alias gb="git branch"
alias gco="git checkout"
alias clone="git clone"
alias cam="git commit -am"
alias gd="git diff"
alias glg="git lg"
alias pull="git pull"
alias push="git push"
alias remote="git remote"
alias status="git status -sb"
alias gmdm="git checkout master && git merge develop master && git push && git checkout develop && git push"
